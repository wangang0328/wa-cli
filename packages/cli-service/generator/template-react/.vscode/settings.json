{
  /*解决vscode别名不提示问题*/
  "path-intellisense.mappings": {
    "@src": "${workspaceRoot}/src"
  },
  "editor.tabSize": 2,
  "eslint.options": {
    "extensions": [
      ".js",
      "jsx",
      ".ts",
      ".tsx",
      ".mjs"
    ]
  },
  /*eslint校验问题在代码上标红*/
  "eslnit.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}